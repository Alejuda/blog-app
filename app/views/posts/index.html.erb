<%= render "shared/profile", user: @user %>
<ul class="posts-container">
  <% @posts.each do |post| %>
    <%= render "shared/post", post: post %>
    <% unless post.comments.empty? %>
      <%= render "shared/comments", comments: post.comments %>
    <% end %>
  <% end %>
  <button class="button"> Pagination </button>
</ul>
